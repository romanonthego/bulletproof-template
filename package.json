  "scripts": {
    "lint": "eslint app/** || true",
    "build": "rm -rf ./build/* && bash scripts/postpublish.sh",
    "start": "nf start",
    "start-dev": "webpack --config webpack-configs/build.development.babel.js && webpack-dashboard -- nf start",
    "start-production": "node build/server.js",
    "start-staging": "node build/server.js",
    "build-production": "node --max_old_space_size=4096 $(npm bin)/webpack --config webpack-configs/build.production.babel.js",
    "build-staging": "node --max_old_space_size=4096 $(npm bin)/webpack --config webpack-configs/build.staging.babel.js",
    "unstore-uploadcare-files": "node scripts/unstore-uploadcare-files",
    "upload-source-maps": "node scripts/upload-source-maps-to-sentry",
    "deps:check": "david || true",
    "deps:update": "david u || true",
    "heroku-prebuild": "node scripts/unstore-uploadcare-files.js",
    "heroku-postbuild": "rm -rf ./build/* && bash scripts/postpublish.sh && rm -rf ./app"
  },
  "cacheDirectories": [
    "node_modules",
    "build-cache"
