    "upload-source-maps": "node scripts/upload-source-maps-to-sentry",
    "deps:check": "david || true",
    "deps:update": "david u || true",
    "heroku-prebuild": "node scripts/unstore-uploadcare-files.js",
    "heroku-postbuild": "rm -rf ./build/* && bash scripts/postpublish.sh && rm -rf ./app"
